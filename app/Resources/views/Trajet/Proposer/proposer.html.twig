{% extends ':Trajet:layout.html.twig' %}

{% block title %} Proposer-{{ parent() }} {% endblock %}

{% block body %}
    <div id="proposer" class="container">
        {{ include(':Trajet/Proposer:proposer_content.html.twig') }}
    </div>
{% endblock %}


{% block javascripts %}

    {{ parent() }}

    <script src="{{ asset('js/waypoints.js') }}"></script>

    <script>

        $(function() {

            $('.menu-links a').removeClass('active');
            $('#idProposer').addClass('active');


            $('input[inputAutoComplete=on]').each(function() {
                document.getElementById($(this).attr('id')).addEventListener('focusin',function() {
                    AutoComplete($(this).attr('id'));
                });
            });

        });

    </script>




{% endblock %}